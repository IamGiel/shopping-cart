<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h1>Profile Page</h1>
    <hr>
    <h2>My Orders</h2>
    {{#each orders}}
    <div class="panel panel-default">
      <div class="panel-body">
        <ul class="list-group">
          {{#each this.items}}
          <li class="list-group-item">
            <span class="badge">${{this.Price}}</span>

            {{this.item.Title}} | {{this.qty}} Units
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="panel-footer"><strong>Total Price: ${{this.cart.totalPrice}} | {{this.date}}</strong></div>
    </div>
    {{/each}}
  </div>
</div>